<div id="chart" class="col-xs-12 col-md-9">
<div id="auth-button"></div>

<div id='GAselect'>
<div id="view-selector"></div>
<div id="view-selector2"></div>
<div id="view-selector3"></div>
</div>

<h1>瀏覽量</h1>
<div id="timeline"></div>
<h1>工作階段</h1>
<div id="timeline2"></div>
<h1>使用者</h1>
<div id="timeline3"></div>
