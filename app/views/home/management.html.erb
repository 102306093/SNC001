    <td>
	<%= link_to "修改", edit_payer_path(payer.id)  %>
      </td>
		<%end%>
		</table>
		</div>
		
		
    <div id="productdata" class="col-xs-12 col-md-9">
    <table class="table table-bordered">
    <tr style="border:">
      <td>產品名稱</td>
	  <td>價格</td>
	  <td>庫存</td>
	  <td>圖片位址</td>
	  <td>seo描述</td>
	</tr>
	<% @product.each do |product| %>
	<tr>
	  <td><%= product.proname%></td>
	  <td><%= product.price%></td>
	  <td><%= product.qty%></td>
	  <td><%= product.pic1%></td>
	  <td><%= product.seodescription%></td>
	  <td>
