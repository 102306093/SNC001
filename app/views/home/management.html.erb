    <td>
	<%= link_to "修改", edit_product_path(product.id)  %>
      </td>
		<%end%>
		</table>
		</div>
		
	<div id="articledata" class="col-xs-12 col-md-9">
    <table class="table table-bordered">
    <tr style="border:">
      <td>文章標題</td>
