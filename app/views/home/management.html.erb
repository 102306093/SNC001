	  <td>圖片位址</td>
	  <td>hashtag</td>
	  <td>擁有端</td>
	  <td>seo描述</td>
	</tr>
	<% @article.each do |article| %>
	<tr>
	  <td><%= article.title%></td>
	  <td><%= article.introduce%></td>
	  <td><%= article.pic1%></td>
	  <td><%= article.hashtag%></td>
	  <td><%= article.owner%></td>
	  <td><%= article.seodescription%></td>
	  <td>
	<%= button_to "刪除", {:controller => :articles,
      :action => 'destroy', :id => article.id }, :method => :delete %>
